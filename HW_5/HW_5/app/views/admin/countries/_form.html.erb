<b><%= link_to "На главную", admin_countries_path %></b>
<%= form_for [:admin, @country], html: { multipart: true } do | f | %>

  <div class="form_control">
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>


  <div class="form_control">
    <%= f.label :image %>
    <%= f.text_field :image %>
  </div>

  <div class="form_control">
    <%= f.label :avatar %>
    <%= f.file_field :avatar %>
  </div>

  <div class="form-control">
    <%= f.submit 'Save' %>
  </div>
<% end %>
